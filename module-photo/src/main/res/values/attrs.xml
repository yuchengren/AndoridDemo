<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PhotoEditView">
        <attr name="imageMatchPercent" format="float" />
        <attr name="shadowColor" format="color"/>
        <attr name="clipColor" format="color"/>
        <attr name="clipCornerWidth" format="dimension" />
        <attr name="clipCornerLineWidth" format="dimension" />
        <attr name="clipBorderLineWidth" format="dimension"/>
        <attr name="clipSpanLineWidth" format="dimension" />
        <attr name="clipRowSpans" format="integer" />
        <attr name="clipColumnSpans" format="integer" />
        
        <attr name="mode" >
            <enum name="none" value="0" />
            <enum name="graffiti" value="1" />
            <enum name="clip" value="2"/>
        </attr>
    </declare-styleable>
</resources>
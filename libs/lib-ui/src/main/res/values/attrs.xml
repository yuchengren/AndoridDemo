<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="NineGridView">
        <attr name="maxItemCount" format="integer" />
        <attr name="columnsNum" format="integer" />
        <attr name="itemCornerRadius" format="dimension" />
        <attr name="plusDrawable" format="reference" />
        <attr name="plusEnabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="GridImageView">
        <attr name="cornerRadius" format="dimension"/>
        <attr name="ratio" format="float" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeVisible" format="boolean" />
        <attr name="strokeColor" format="color" />
    </declare-styleable>

</resources>